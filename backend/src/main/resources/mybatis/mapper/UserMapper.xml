<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.thirautech.amingwiki.dao.UserDao">
    <select id="findAllUserInfo" resultType="com.thirautech.amingwiki.dto.UserDto">
        SELECT * FROM WIKIUSER;
    </select>
    <select id="findAllUserInfoById" parameterType="string" resultType="com.thirautech.amingwiki.dto.UserDto">
        SELECT *
        FROM WIKIUSER
        WHERE userId = #{id, jdbcType=VARCHAR}
    </select>
</mapper>

